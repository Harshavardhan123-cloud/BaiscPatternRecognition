{
    "builds": [{
        "src": "pattern/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb" , "runtime":"python3.10"}
    },
    {
        "src": "build_file.sh",
        "use": "@vercel/static-build",
        "config":{
            "distDir": "collectstatic"
            }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "pattern/wsgi.py"
        },
        {
            "src":"/static/(.*)",
            "dest":"static/$1"
        }
    ]
}